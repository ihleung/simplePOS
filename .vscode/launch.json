  "version": "0.2.0",
  "configurations": [
    {
      "name": "C/C++: clang++ build and debug active file",
      "type": "cppdbg",
      "request": "launch",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "args": [
        "Stations1.txt",
        "Stations2.txt",
        "CustomerOrders.txt",
        "AssemblyLine.txt"
